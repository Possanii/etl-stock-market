{
  "name": "etl",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "prepare": "husky",
    "clean": "rimraf ./build",
    "build": "npm-run-all lint format clean && tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "eslint": "9.x",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  }
}
